\begin_layout Subsection
Reproductive success through philopatric offspring
\end_layout

\begin_layout Standard
Following eqns.
 A4-A6 from 
\begin_inset CommandInset citation
LatexCommand cite
key "Rodrigues2013"
literal "false"

\end_inset

, the expected number of mutant individuals who live in a big patch at time
 
\begin_inset Formula $t+1$
\end_inset

 given that the focal mutant lives in a big patch at time 
\begin_inset Formula $t$
\end_inset

 is given by
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
E_{B\rightarrow B}^{\text{phil}} & =1-\mu_{B}\left(z_{B}^{\bullet}\right)+\frac{n_{B}\mu_{B}\left(z_{B}^{\circ},z_{B}^{\circ}\right)\left(1-d_{B}\right)f\left(z_{B}^{\bullet},z_{B}^{\circ}\right)}{n_{B}\left(1-d_{B}\right)f\left(z_{B}^{\circ},z_{B}^{\circ}\right)+p_{B}n_{B}d_{B}f\left(z_{B},z_{B}\right)+p_{L}n_{L}d_{L}f\left(z_{L},z_{L}\right)}.
\end{align*}

\end_inset

Next, we calculate the expected number 
\begin_inset Formula $E_{B\rightarrow L}^{\text{phil}}$
\end_inset

 of philopatric mutant individauls who are able to remain or establish as
 breeders when the patch transitions from a big patch to a little patch.
 To this end, the probability 
\begin_inset Formula $\mathcal{P}\left(k\mid n_{B},\mu_{B}\left(z_{B}^{\circ},z_{B}^{\circ}\right)\right)$
\end_inset

 that 
\begin_inset Formula $0\leq k\leq n_{B}$
\end_inset

 surviving breeders will be sampled (potentially including the focal) out
 of the original number of 
\begin_inset Formula $n_{B}$
\end_inset

 breeders according to their survival probability 
\begin_inset Formula $1-\mu_{B}(z_{B}^{\circ},z_{B}^{\circ})$
\end_inset

 is given by the binomial distribution
\begin_inset Formula 
\begin{align*}
\mathcal{P}\left(k\mid n_{B},\mu_{B}\left(z_{B}^{\circ},z_{B}^{\circ}\right)\right) & ={n_{B} \choose k}\left[1-\mu_{B}\left(z_{B}^{\circ},z_{B}^{\circ}\right)\right]^{k}\left[\mu_{B}\left(z_{B}^{\circ},z_{B}^{\circ}\right)\right]^{n_{B}-k}.
\end{align*}

\end_inset

If 
\begin_inset Formula $k<n_{L}$
\end_inset

, we sample new recruits to fill the 
\begin_inset Formula $n_{L}-k$
\end_inset

 vacancies.
 If 
\begin_inset Formula $k=n_{L}$
\end_inset

, all 
\begin_inset Formula $n_{L}$
\end_inset

 breeding positions are occupied by surviving adults and no new recruits
 are sampled.
 If 
\begin_inset Formula $n_{L}<k\leq n_{B}$
\end_inset

, then surviving adults compete among themselves for breeding positions.
 The expected number 
\begin_inset Formula $E_{B\rightarrow L}^{\text{phil}}$
\end_inset

 of mutant individuals who live in a little patch at time 
\begin_inset Formula $t+1$
\end_inset

 given that the focal mutant lives in a big patch at time 
\begin_inset Formula $t$
\end_inset

 is then given by
\begin_inset Formula 
\begin{align*}
E_{B\rightarrow L}^{\text{phil}} & =\sum_{k=1}^{n_{B}}\mathcal{P}\left(k-1\mid n_{B}-1,\mu_{B}\left(z_{B}^{\circ},z_{B}^{\circ}\right)\right)\left(1-\mu_{B}\left(z_{B}^{\bullet}\right)\right)\left[\mathrm{min}\left\{ \frac{n_{L}}{k},1\right\} +\frac{\delta_{k\leq n_{\mathrm{L}}}\left(n_{L}-k\right)\left(1-d_{B}\right)f\left(z_{B}^{\bullet},z_{B}^{\circ}\right)}{n_{B}\left(1-d_{B}\right)f\left(z_{B}^{\circ},z_{B}^{\circ}\right)+p_{B}n_{B}d_{B}f\left(z_{B},z_{B}\right)+p_{L}n_{L}d_{L}f\left(z_{L},z_{L}\right)}\right]\\
 & +\sum_{k=0}^{n_{B}-1}\mathcal{P}\left(k\mid n_{B}-1,\mu_{B}\left(z_{B}^{\circ},z_{B}^{\circ}\right)\right)\mu_{B}\left(z_{B}^{\bullet}\right)\left[\frac{\delta_{k\leq n_{\mathrm{L}}}\left(n_{L}-k\right)\left(1-d_{B}\right)f\left(z_{B}^{\bullet},z_{B}^{\circ}\right)}{n_{B}\left(1-d_{B}\right)f\left(z_{B}^{\circ},z_{B}^{\circ}\right)+p_{B}n_{B}d_{B}f\left(z_{B},z_{B}\right)+p_{L}n_{L}d_{L}f\left(z_{L},z_{L}\right)}\right]
\end{align*}

\end_inset

where the first line reflects the case in which the focal adult survives,
 with probability 
\begin_inset Formula $1-\mu_{B}\left(z_{B}^{\bullet}\right)$
\end_inset

, in which case it successfully competes with 
\begin_inset Formula $k$
\end_inset

 adults (including itself) over 
\begin_inset Formula $n_{L}$
\end_inset

 vacant breeding positions.
 The 
\begin_inset Formula $\mathrm{min}\{\}$
\end_inset

 denotes that if 
\begin_inset Formula $n_{L}>k$
\end_inset

, the focal's probability of obtaining the vacant breeding position is 
\begin_inset Formula $1$
\end_inset

.
 The second term in parentheses reflects competition by juveniles over the
 
\begin_inset Formula $n_{L}-k$
\end_inset

 breeding positions that are vacant after adults compete over the 
\begin_inset Formula $n_{L}$
\end_inset


\end_layout

\begin_layout Standard
Calculating the expected number 
\begin_inset Formula $E_{L\rightarrow B}^{\text{phil}}$
\end_inset

 of philopatric mutant individuals who are able to remain or establish as
 breeders when the patch transitions from a little patch to a big patch
 is more straightforward: in this case, all surviving breeders retain their
 breeding position.
 Any additional vacancies are filled by new recruits.
 Hence we have 
\begin_inset Formula 
\begin{align*}
E_{L\rightarrow B}^{\text{phil}} & =1-\mu_{L}\left(z_{L}^{\bullet},z_{L}^{\circ}\right)+\frac{\left[n_{B}-n_{L}+n_{L}\mu_{L}\left(z_{L}^{\circ},z_{L}^{\circ}\right)\right]\left(1-d_{L}\right)f\left(z_{L}^{\bullet},z_{L}^{\circ}\right)}{n_{L}\left(1-d_{L}\right)f\left(z_{L}^{\circ},z_{L}^{\circ}\right)+p_{B}n_{B}d_{B}f\left(z_{B},z_{B}\right)+p_{L}n_{L}d_{L}f\left(z_{L},z_{L}\right)}
\end{align*}

\end_inset

Finally, we have the expected number 
\begin_inset Formula $E_{L\rightarrow L}^{\bullet}$
\end_inset

 of philopatric mutant individuals who are able to remain or establish as
 breeders when the patch remains little:
\begin_inset Formula 
\begin{align*}
E_{L\rightarrow L}^{\text{phil}} & =1-\mu_{L}\left(z_{L}^{\bullet},z_{L}^{\circ}\right)+\frac{n_{L}\mu_{L}\left(z_{L}^{\circ},z_{L}^{\circ}\right)\left(1-d_{L}\right)f\left(z_{L}^{\bullet},z_{L}^{\circ}\right)}{n_{L}\left(1-d_{L}\right)f\left(z_{L}^{\circ},z_{L}^{\circ}\right)+p_{B}n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{L}n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Reproductive success through dispersing offspring
\end_layout

\begin_layout Standard
To assess the fate of mutant offspring in a remote patch, whether the site
 has recently been a large vs small patch becomes important, as this affects
 the number of breeding positions available.
 Using Bayes' Theorem, let 
\begin_inset Formula $q_{U\mid V}$
\end_inset

 reflect the probability that a patch in environmental state 
\begin_inset Formula $E_{t+1}=V$
\end_inset

 at time 
\begin_inset Formula $t+1$
\end_inset

 was in state 
\begin_inset Formula $E_{t}=U$
\end_inset

 at time 
\begin_inset Formula $t$
\end_inset

:
\begin_inset Formula 
\begin{align*}
q_{U\rightarrow V} & =\Pr\left(E_{t}=U\mid E_{t+1}=V\right)=\frac{\Pr\left(E_{t+1}=V\mid E_{t}=U\right)\Pr\left(E_{t}=U\right)}{\Pr\left(E_{t+1}=V\right)}=\frac{p_{U}s_{U\rightarrow V}}{\sum_{x\in\{U,V\}}p_{x}s_{x\rightarrow V}}.
\end{align*}

\end_inset

We then have
\begin_inset Formula 
\begin{align*}
E_{B\rightarrow B}^{\text{disp}} & =d_{B}q_{B\rightarrow B}\frac{n_{B}\mu_{B}\left(z_{B},z_{B}\right)f\left(z_{B}^{\bullet},z_{B}^{\circ}\right)}{n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{B}n_{B}d_{B}f\left(z_{B},z_{B}\right)+p_{L}n_{L}d_{L}f\left(z_{L},z_{L}\right)}\\
 & +d_{B}q_{L\rightarrow B}\frac{\left[n_{B}-n_{L}+n_{L}\mu_{L}\left(z_{L},z_{L}\right)\right]f\left(z_{B}^{\bullet},z_{B}^{\circ}\right)}{n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)+p_{B}n_{B}d_{B}f\left(z_{B},z_{B}\right)+p_{L}n_{L}d_{L}f\left(z_{L},z_{L}\right)}
\end{align*}

\end_inset

Next, 
\begin_inset Formula 
\begin{align*}
E_{B\rightarrow L}^{\text{disp}} & =d_{B}q_{B\rightarrow L}\sum_{k=0}^{n_{B}}\mathcal{P}\left(k\mid n_{B},\mu_{B}\left(z_{B},z_{B}\right)\right)\frac{\delta_{k\leq n_{\mathrm{L}}}\left(n_{L}-k\right)f\left(z_{B}^{\bullet},z_{B}^{\circ}\right)}{n_{B}\left(1-d_{B}\right)f_{B}\left(z_{B},z_{B}\right)+p_{B}n_{B}d_{B}f\left(z_{B},z_{B}\right)+p_{L}n_{L}d_{L}f\left(z_{L},z_{L}\right)}\\
 & +d_{B}q_{L\rightarrow L}\frac{n_{L}\mu_{L}\left(z_{L},z_{L}\right)f\left(z_{B}^{\bullet},z_{B}^{\circ}\right)}{n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)+p_{B}n_{B}d_{B}f\left(z_{B},z_{B}\right)+p_{L}n_{L}d_{L}f\left(z_{L},z_{L}\right)},
\end{align*}

\end_inset

And
\begin_inset Formula 
\begin{align*}
E_{L\rightarrow B}^{\mathrm{disp}} & =d_{L}q_{B\rightarrow B}\frac{n_{B}\mu_{B}\left(z_{B},z_{B}\right)f\left(z_{L}^{\bullet},z_{L}^{\circ}\right)}{n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{B}n_{B}d_{B}f\left(z_{B},z_{B}\right)+p_{L}n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}\\
 & +d_{L}q_{L\rightarrow B}\frac{\left[n_{B}-n_{L}+n_{L}\mu_{L}\left(z_{L},z_{L}\right)\right]f\left(z_{L}^{\bullet},z_{L}^{\circ}\right)}{n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)+p_{B}n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{L}n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}.
\end{align*}

\end_inset

Finally,
\begin_inset Formula 
\begin{align*}
E_{L\rightarrow L}^{\text{disp}} & =d_{L}q_{B\rightarrow L}\sum_{k=0}^{n_{L}}\mathcal{P}\left(k\mid n_{B},\mu_{L}\left(z_{L},z_{L}\right)\right)\frac{\left(n_{L}-k\right)f\left(z_{L}^{\bullet},z_{L}^{\circ}\right)}{n_{B}\left(1-d_{B}\right)f_{B}\left(z_{B},z_{B}\right)+p_{B}n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{L}n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}\\
 & +d_{L}q_{L\rightarrow L}\frac{n_{L}\mu_{L}\left(z_{L},z_{L}\right)f\left(z_{L}^{\bullet},z_{L}^{\circ}\right)}{n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)+p_{B}n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{L}n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}.
\end{align*}

\end_inset

From the above we can then compile the matrix 
\begin_inset Formula $(e_{ij})\equiv\mathbf{E}$
\end_inset

 reflecting the expected number of focal alleles contained in individuals
 that are successfully established in a patch in state 
\begin_inset Formula $i\in B,L$
\end_inset

 resulting from a focal individual currently breeding in a patch in state
 
\begin_inset Formula $j\in B,L$
\end_inset

.
 We then have
\begin_inset Formula 
\begin{align*}
\mathbf{E} & =\mathbf{E}^{\mathrm{phil}}+\mathbf{E}^{\mathrm{disp}},
\end{align*}

\end_inset

where
\begin_inset Formula 
\begin{align*}
\mathbf{E}^{\mathrm{i}} & =\left[\begin{array}{cc}
E_{B\rightarrow B}^{\mathrm{i}} & E_{L\rightarrow B}^{\mathrm{i}}\\
E_{B\rightarrow L}^{\mathrm{i}} & E_{L\rightarrow L}^{\mathrm{i}}
\end{array}\right]
\end{align*}

\end_inset

for 
\begin_inset Formula $\mathrm{i}\in\mathrm{disp,phil}.$
\end_inset

 The expected fitness 
\begin_inset Formula $\mathbf{W}$
\end_inset

 of the focal mutant is then the matrix product of the matrix 
\begin_inset Formula $(s_{ij})\equiv\mathbf{S}$
\end_inset

 containing the probabilities 
\begin_inset Formula $s_{j\rightarrow i}$
\end_inset

 that a patch may change in size and 
\begin_inset Formula $\mathbf{E}$
\end_inset

 or
\begin_inset Formula 
\begin{align*}
\mathbf{W} & =\mathbf{S}\mathbf{E},
\end{align*}

\end_inset

where
\begin_inset Formula 
\begin{align*}
\mathbf{S} & =\left[\begin{array}{cc}
s_{B\rightarrow B} & 1-s_{L\rightarrow L}\\
1-s_{B\rightarrow B} & s_{L\rightarrow L}
\end{array}\right].
\end{align*}

\end_inset

According to standard results, the autocorrelation 
\begin_inset Formula $\rho$
\end_inset

 in patch state is given by 
\begin_inset Formula $\rho=1-s_{B\rightarrow B}-s_{L\rightarrow L}$
\end_inset

, while the relative frequency 
\begin_inset Formula $p_{B}$
\end_inset

 of the large patch is given by 
\begin_inset Formula $p_{B}=\left(1-s_{L\rightarrow L}\right)\left/\left(1-s_{L\rightarrow L}+1-s_{B\rightarrow B}\right)\right.$
\end_inset

.
 
\end_layout

