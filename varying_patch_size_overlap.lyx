\begin_layout Subsection
Reproductive values and stable class frequencies
\end_layout

\begin_layout Standard
Reproductive values can be calculated by taking the dominant left eigenvector
 of the matrix 
\end_layout

\begin_layout Subsection
Relatedness coefficients
\end_layout

\begin_layout Standard
The coefficient of consanguinity 
\begin_inset Formula $Q_{X}$
\end_inset

 between two randomly sampled adult breeders in a patch in state 
\begin_inset Formula $X\in\{B,L\}$
\end_inset

 is given by the following recursions:
\begin_inset Formula 
\begin{align*}
Q_{L,t+1} & =q_{L\rightarrow L}Q_{L\rightarrow L,t}^{\prime}+q_{B\rightarrow L}Q_{B\rightarrow L,t}^{\prime}\\
Q_{B,t+1} & =q_{L\rightarrow B}Q_{L,t}^{\prime}+q_{B\mid B}Q_{B,t}^{\prime}.
\end{align*}

\end_inset

Here, 
\begin_inset Formula $Q_{X\rightarrow Y}^{\prime}$
\end_inset

 reflects the coefficient of consanguinity among two randomly sampled adult
 breeders in a patch in state 
\begin_inset Formula $X$
\end_inset

 before it changes to environment 
\begin_inset Formula $Y$
\end_inset

.
 Starting with 
\begin_inset Formula $Q_{L\rightarrow L,t}^{\prime}$
\end_inset

, the coefficient of consanguinity between two randomly sampled adult breeders
 in a little patch that remains a little patch is given by the recursion
\begin_inset Formula 
\begin{align*}
Q_{L\rightarrow L,t}^{\prime} & =\left(1-\mu_{L}\left(z_{L},z_{L}\right)\right)^{2}Q_{L,t}\\
 & +2\left(1-\mu_{L}\left(z_{L},z_{L}\right)\right)\mu_{L}\left(z_{L},z_{L}\right)g_{L\rightarrow L}\left(\frac{1}{n_{L}}+\frac{n_{L}-1}{n_{L}}Q_{L,t}\right)\\
 & +\mu_{L}\left(z_{L},z_{L}\right)^{2}g_{L\rightarrow L}^{2}\left(\frac{1}{n_{L}}+\frac{n_{L}-1}{n_{L}}Q_{L,t}\right).
\end{align*}

\end_inset

Here, 
\begin_inset Formula $g_{L\rightarrow L}$
\end_inset

 reflects the probability that the sampled juvenile who is recruited to
 the little patch (which remains little) is philopatric:
\begin_inset Formula 
\begin{align*}
g_{L\rightarrow L} & =\frac{n_{L}\mu_{L}\left(z_{L},z_{L}\right)\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}{n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)+p_{B}n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{L}n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}.
\end{align*}

\end_inset

Next, 
\begin_inset Formula $Q_{B\rightarrow L}^{\prime}$
\end_inset

 reflects the coefficient of consanguinity between two randomly sampled
 adult breeders in a little patch that was previously a big patch:
\begin_inset Formula 
\begin{align*}
Q_{B\rightarrow L}^{\prime} & =\sum_{k=0}^{n_{B}}\mathcal{P}\left(k\mid n_{B},\mu_{B}\left(z_{B},z_{B}\right)\right)\left[\delta_{k\leq n_{L}}\left(\frac{k}{n_{L}}\frac{k-1}{n_{L}-1}Q_{B,t}\right)+\left(1-\delta_{k\leq n_{L}}\right)Q_{B,t}\right.\\
 & +\delta_{k\leq n_{L}-1}g_{B\rightarrow L}(k)\left(\frac{k}{n_{L}}\frac{n_{L}-\left(k-1\right)}{n_{L}-1}+\frac{n_{L}-k}{n_{L}}\frac{k}{n_{L}-1}\right)\left(\frac{1}{k}+\frac{k-1}{k}Q_{B,t}\right)\\
 & \left.+\delta_{k\leq n_{L}-2}g_{B\rightarrow L}(k)^{2}\left(\frac{n_{L}-k}{n_{L}}\frac{n_{L}-k-1}{n_{L}-1}\right)\left(\frac{1}{n_{B}}+\frac{n_{B}-1}{n_{B}}Q_{B,t}\right)\right].
\end{align*}

\end_inset

Note that the recursion in the second line involves the term 
\begin_inset Formula $[1/k+(k-1)/k\times Q_{B,t}]$
\end_inset

 which reflects the probability that one of the sampled individuals is either
 the newborn's parent or alternatively a relative.
 By contrast, the recursion in the third line involves the term 
\begin_inset Formula $[1/n_{B}+(n_{B}-1)/n_{B}\times Q_{B,t}]$
\end_inset

, reflecting the probability that both sampled newborns share the same parent
 (which is one of the 
\begin_inset Formula $n_{B}$
\end_inset

 parents that previously lived in the patch) or do not share the same parent
 (with probability 
\begin_inset Formula $(n_{B}-1)/n_{B}$
\end_inset

, in which case the coefficient of consanguinity is given by 
\begin_inset Formula $Q_{B,t}$
\end_inset

.
 The function 
\begin_inset Formula $g_{B\rightarrow L}(k)$
\end_inset

 reflects the probability that the sampled juvenile who is recruited to
 a previously big, but now little patch with 
\begin_inset Formula $k<n_{L}$
\end_inset

 surviving adults is philopatric to the local patch and is given by
\begin_inset Formula 
\begin{align*}
g_{B\rightarrow L}\left(k\right) & =\frac{\left(n_{L}-k\right)\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)}{n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{B}n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{L}n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}\quad k<n_{L}.
\end{align*}

\end_inset

Next, 
\begin_inset Formula $Q_{L\rightarrow B}^{\prime}$
\end_inset

 reflects the coefficient of consanguinity between two randomly sampled
 adult breeders in a big patch that was previously a little patch:
\begin_inset Formula 
\begin{align*}
Q_{L\rightarrow B}^{\prime} & =\sum_{k=0}^{n_{L}}\mathcal{P}\left(k\mid n_{L},\mu_{L}\left(z_{L},z_{L}\right)\right)\left[\frac{k}{n_{B}}\frac{k-1}{n_{B}-1}Q_{L,t}\right.\\
 & +g_{L\rightarrow B}(k)\left(\frac{k}{n_{B}}\frac{n_{B}-\left(k-1\right)}{n_{B}-1}+\frac{n_{B}-k}{n_{B}}\frac{k}{n_{B}-1}\right)\left(\frac{1}{k}+\frac{k-1}{k}Q_{L,t}\right)\\
 & \left.+g_{L\rightarrow B}(k)^{2}\left(\frac{n_{B}-k}{n_{B}}\frac{n_{B}-k-1}{n_{B}-1}\right)\left(\frac{1}{n_{L}}+\frac{n_{L}-1}{n_{L}}Q_{L,t}\right)\right],
\end{align*}

\end_inset

where 
\begin_inset Formula $g_{L\rightarrow B}(k)$
\end_inset

 is given by
\begin_inset Formula 
\begin{align*}
g_{L\rightarrow B}(k) & =\frac{\left(n_{B}-n_{L}+k\right)\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}{n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)+p_{B}n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{L}n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}.
\end{align*}

\end_inset

Finally, 
\begin_inset Formula $Q_{B\rightarrow B}^{\prime}$
\end_inset

 is given by
\begin_inset Formula 
\begin{align*}
Q_{B\rightarrow B}^{\prime} & =\left[1-\mu_{B}\left(z_{B},z_{B}\right)\right]^{2}Q_{B,t}\\
 & +2\mu_{B}\left(z_{B},z_{B}\right)\left[1-\mu_{B}\left(z_{B},z_{B}\right)\right]g_{B\rightarrow B}\left(\frac{1}{n_{B}}+\frac{n_{B}-1}{n_{B}}Q_{B,t}\right)\\
 & +\mu_{B}\left(z_{B},z_{B}\right)^{2}g_{B\rightarrow B}\left(\frac{1}{n_{B}}+\frac{n_{B}-1}{n_{B}}Q_{B,t}\right),
\end{align*}

\end_inset

where
\begin_inset Formula 
\begin{align*}
g_{B\rightarrow B} & =\frac{n_{B}\mu_{B}\left(z_{B},z_{B}\right)\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)}{n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{B}n_{B}\left(1-d_{B}\right)f\left(z_{B},z_{B}\right)+p_{L}n_{L}\left(1-d_{L}\right)f\left(z_{L},z_{L}\right)}.
\end{align*}

\end_inset


